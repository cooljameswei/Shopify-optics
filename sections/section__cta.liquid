<style>
{% render 'common-css', section: section %}

{% if section.settings.overlayOpacity > 0 %}
.overlay--{{-section.id}} {
    opacity: {{section.settings.overlayOpacity | times: 0.01}};
    background-color: {{section.settings.overlayColor}};
}
{% endif %}

.max-width--content {
  position: relative;
}

@media screen and (min-width: 768px){
  #maxWidth--{{-section.id}}{
    max-width: {{section.settings.maxContentWidth-}}%;
  }
}

#colorcode{}
</style>

<div
id="s--{{-section.id}}"
{% unless section.settings.backgroundImage == nil -%}{% render 'background', image: section.settings.backgroundImage %}{%- endunless %}
class="{% render 'common-class', section: section, class: 'row', bg: true %}">
  {% if section.settings.overlayOpacity > 0 %}
    <div
    class="overlay overlay--{{-section.id}}"
    style="opacity: {{section.settings.overlayOpacity | times: 0.01}}; background-color: {{section.settings.overlayColor}};">
    </div>
  {% endif %}
  <div 
  class="row no-gap {{ section.settings.horizontalAlignment }} layout__content s__content">
    <div id="maxWidth--{{-section.id}}" class="row max-width--content {{ section.settings.horizontalAlignment }}">
      {% render 'block__general--heading' with section as section %}
      {%- for block in section.blocks %}
        {% if block.type == '@app' %}
          <div 
            class="s__block s__block--{{-block.type}} col-12 no-gap {{ section.settings.horizontalAlignment }}">
            {% render block %}
          </div>
          {% continue %}
        {% endif %}
        <div 
        class="s__block s__block--{{-block.type}} {%if block.settings.wideButton or block.type == 'separator' or block.settings.fullWidth%}col-12 no-gap {{ section.settings.horizontalAlignment }}{%else%}margin__horizontal--4{%endif-%}">
          {% render 'get-block' with block as block %}
        </div>
      {% endfor %}
    </div>
  </div>
</div>

