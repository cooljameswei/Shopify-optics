        if(destroy && $('.zoom-container').length > 0){
          if($('.zoom-container').zoom){
            $('.zoom-container').trigger('zoom.destroy');  
          }
        }
        if($('.zoom-container').length > 0){
          $('.zoom-container')
          .css('display', 'block')
          .zoom({
            url:$(".image-zoom").attr('data-zoom')
          });
        }else{
          $('.image-zoom')
