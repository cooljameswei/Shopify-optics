  /*--- Unslick the vertical thumbs ---*/
  function unslickTray(mainHeight) {
    if ($(traySlider).hasClass('slick-slider')) {
      $(traySlider).slick('slickUnfilter');
      $(traySlider).slick('unslick');
    }
