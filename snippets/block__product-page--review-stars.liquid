<div class="product__review-stars {% if block.settings.noMargin or noMargin %}margin__bottom--reduced{%endif-%}" {{block.shopify_attributes}}>
{% case settings.reviewApp %}
    {% when 'shopifyReviews' %}
        <span class="shopify-product-reviews-badge" data-id="{{ product.id }}"></span>
    {% when 'booster' %}
        {% assign totalStars = 5 %}
        {% assign stars = '0' %}
        {% for tag in product.tags %}
            {% if tag contains 'bstr-stars-' %}
                {% assign tempstars = tag | split: '-' | last %}
                {% assign stars = tempstars | at_most: 5 %}
                {% break %}
            {% endif %}
        {% endfor %}
        <span class="card__reviews disable-select">
            {% assign totalStars = totalStars | minus: stars %}
            {% for star in (1..stars) %}
                <i class="card__reviews--positive"><?xml version="1.0" encoding="UTF-8" standalone="no"?><svg width="21px" height="20px" viewBox="0 0 21 20" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><g stroke="none" stroke-width="1" fill="currentColor" fill-rule="evenodd"><g transform="translate(-99.000000, -320.000000)"><g transform="translate(56.000000, 160.000000)"><path d="M60.556381,172.206 C60.1080307,172.639 59.9043306,173.263 60.0093306,173.875 L60.6865811,177.791 C60.8976313,179.01 59.9211306,180 58.8133798,180 C58.5214796,180 58.2201294,179.931 57.9282291,179.779 L54.3844766,177.93 C54.1072764,177.786 53.8038262,177.714 53.499326,177.714 C53.1958758,177.714 52.8924256,177.786 52.6152254,177.93 L49.0714729,179.779 C48.7795727,179.931 48.4782224,180 48.1863222,180 C47.0785715,180 46.1020708,179.01 46.3131209,177.791 L46.9903714,173.875 C47.0953715,173.263 46.8916713,172.639 46.443321,172.206 L43.575769,169.433 C42.4480682,168.342 43.0707186,166.441 44.6289197,166.216 L48.5916225,165.645 C49.211123,165.556 49.7466233,165.17 50.0227735,164.613 L51.7951748,161.051 C52.143775,160.35 52.8220755,160 53.499326,160 C54.1776265,160 54.855927,160.35 55.2045272,161.051 L56.9769285,164.613 C57.2530787,165.17 57.7885791,165.556 58.4080795,165.645 L62.3707823,166.216 C63.9289834,166.441 64.5516338,168.342 63.423933,169.433 L60.556381,172.206 Z"></path></g></g></g></svg></i>
            {% endfor %}
            {% for star in (1..totalStars) %}
                <i class="card__reviews--empty"><?xml version="1.0" encoding="UTF-8" standalone="no"?><svg width="21px" height="20px" viewBox="0 0 21 20" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"><g stroke="none" stroke-width="1" fill="currentColor" fill-rule="evenodd"><g transform="translate(-139.000000, -320.000000)" fill="currentColor"><g transform="translate(56.000000, 160.000000)"><path d="M98.7860271,170.404 C97.7402268,171.416 97.2624767,172.872 97.5102768,174.301 L97.9691269,176.957 L95.5646263,175.703 C94.9304261,175.372 94.216426,175.197 93.5003258,175.197 C92.7831756,175.197 92.0691754,175.372 91.4349753,175.703 L89.0304747,176.957 L89.4903748,174.301 C89.7371249,172.872 89.2593748,171.416 88.2135745,170.404 L86.268974,168.523 L88.9569747,168.135 C90.402825,167.927 91.6512753,167.027 92.2980755,165.727 L93.5003258,163.31 L94.7025761,165.727 C95.3483262,167.027 96.5978265,167.927 98.0426269,168.135 L100.731678,168.523 L98.7860271,170.404 Z M103.423878,169.433 C104.551578,168.342 103.929978,166.441 102.370728,166.216 L98.408027,165.645 C97.7885268,165.556 97.2530267,165.17 96.9768766,164.613 L95.2044762,161.051 C94.8558761,160.35 94.1775759,160 93.5003258,160 C92.8220256,160 92.1437255,160.35 91.7951254,161.051 L90.0237749,164.613 C89.7465749,165.17 89.2110747,165.556 88.5926246,165.645 L84.6299236,166.216 C83.0706733,166.441 82.4480231,168.342 83.5757234,169.433 L86.4432741,172.206 C86.8916242,172.639 87.0963742,173.263 86.9903242,173.875 L86.313074,177.791 C86.102024,179.01 87.0785242,180 88.1862745,180 C88.4781746,180 88.7805746,179.931 89.0714247,179.779 L92.6151756,177.93 C92.8923756,177.786 93.1958257,177.713 93.5003258,177.713 C93.8037759,177.713 94.1072259,177.786 94.384426,177.93 L97.9292269,179.779 C98.2200769,179.931 98.521427,180 98.8133271,180 C99.9210773,180 100.897578,179.01 100.686528,177.791 L100.010327,173.875 C99.9042773,173.263 100.107977,172.639 100.556327,172.206 L103.423878,169.433 Z" id="star_favorite-[#1498]"></path></g></g></g></svg></i>
            {% endfor %}
        </span>
    {% when 'loox' %}
        <a href="#looxReviews">
            <div class="loox-rating" data-id="{{ product.id }}" data-rating="{{ product.metafields.loox.avg_rating }}" data-raters="{{ product.metafields.loox.num_reviews }}">
            </div>
        </a>
    {% when 'alireviews' %}
        <div product-id="{{ product.id }}" class="alr-display-review-badge"></div>
{% endcase %}
</div>

{% comment %}
{% blockdef %}

{% endblockdef %}
{% endcomment %}