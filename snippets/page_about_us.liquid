.page-width{
	width: 100%;
    margin: auto;
    /*max-width: 130rem;*/
	/*------As per the new requiremnet TW#33265426-----*/
    max-width: 1350px;
    color: #666666;
    padding: 0 2rem;
}
.main-content{
  margin-top: 30px;
}
.main-content div:not(.booster--revert)>h1.small--text-center,
.main-content .page-content-width h1.small--text-center a{
	font-family: Lato,sans-serif;
}
.main-content .page-content-width a{
	color:black;
}
.bg-transparent {
	background: transparent !important;
}
.main-content div.red:not(.booster--revert)>h3:first-child{
	font-size:1.5em;
	color:#c63b3d;
}
.main-content div.red:not(.booster--revert)>p{
	color:#c63b3d;	
}
.main-content .page-content-width .about-contact a:hover{
	background-color:#99bc46;
	color:white !important;
}
.shopify-section.custom-page-content-section.custom-shipping-section .interior-block ol.spacing li{
	line-height:1.375;
}
.about-contact a:hover{
	color:white !important;
}
@media only screen and (min-width: 750px){
	h1 {
    	font-size: 2.25em;
	}
	
}
@media(max-width:767px){
	.interior-block .row{
		margin:0px;
	}
	.main-content .shopify-section .red-section-bg ul{
		padding:0px;
	}
	.wrapper--bottom{
		padding-top:10px;
	}
}
@media(max-width:750px){
	ol.spacing {
      display: block;
      clear: both;
      padding-top: 25px;
	}	
}
