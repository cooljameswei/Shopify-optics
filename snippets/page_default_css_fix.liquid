.main-content {
    margin-top: 30px;
}
h2{
	margin-top:40px;
}
.page-content-width.default-page .content-block .title h3{
	margin-bottom:0px;
}
.page-content-width.default-page .content-block h3 {
    font-size: 1.5em;
	line-height: 1.1;
	margin: 0 0 .625em;
}
@media only screen and (max-width: 749px){
  .page-content-width.default-page{
	padding:0px 15px;
  }
  .page-content-width.default-page>h1.page-title, .default-page div:not(.booster--revert)>h1.page-title {
      text-align: center;
	  font-size: 1.875em;	
  }
  .wrapper--bottom{
	padding-top:15px;
  }
  	
}