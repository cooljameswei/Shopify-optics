  .modal {
    display: none; 
    position: fixed;
    padding-top: 100px; 
    left: 50%;
    top: 30%;
    width: auto;
    overflow: auto; 
    opacity: 1;
    padding-top: 0;
    background-color: transparent;
    transition: all ease-in-out .2s;
    line-height: 1.375;
    font-family: Karla,HelveticaNeue,"Helvetica Neue",sans-serif;
    transform: translate(-50%, -30%);
    z-index: 99;
    bottom: initial;
    right: initial;
    height: auto;
    color: #666;
  }
  
