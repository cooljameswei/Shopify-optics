      "blocks": {
        "c6ebf9e7-43ac-435e-8d9f-efb4703eaa78": {
          "type": "ItemList",
          "settings": {
            "item_icon": "shopify:\/\/shop_images\/icon__light_bulb.png",
            "item_text": "<p>Progressive glasses can take time to get used to — from three to almost a dozen days! Luckily, our 2-week return policy lets you adjust to them at your own pace, worry-free.<\/p>"
          }
        }
      },
