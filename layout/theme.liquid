      {%- elsif template contains "product" -%}
        {% capture optimized_content_for_header %}{{ content_for_header }}{% endcapture %}  
        {{ optimized_content_for_header | remove:'"https:\/\/orderstatus.w3apps.co\/js\/orderlookup.js?shop=designeroptics-com.myshopify.com",' | remove:'"https:\/\/cdn.kilatechapps.com\/media\/countdown\/28078986.min.js?shop=designeroptics-com.myshopify.com",' | remove:'"https:\/\/productoptions.w3apps.co\/js\/options.js?shop=designeroptics-com.myshopify.com",' | remove:'"https:\/\/wrapin.prezenapps.com\/public\/js\/load-wrapin-app.js?shop=designeroptics-com.myshopify.com",' | remove:'"https:\/\/egw-app.herokuapp.com\/get_script\/605d7ba0d21f11eca6aa629f55abbb69.js?v=608206\u0026shop=designeroptics-com.myshopify.com",' | remove:'"https:\/\/account-invite.merchantyard.com\/js\/integrations\/script.js?v=6543\u0026shop=designeroptics-com.myshopify.com",' | remove:'"https:\/\/services.nofraud.com\/js\/device.js?shop=designeroptics-com.myshopify.com",' | remove:'"https:\/\/cdn.shopify.com\/shopifycloud\/privacy-banner\/storefront-banner.js?shop=designeroptics-com.myshopify.com",' | remove:'"https:\/\/cdn.506.io\/eg\/script.js?shop=designeroptics-com.myshopify.com",' | remove:'"https:\/\/cdn.attn.tv\/do\/dtag.js?shop=designeroptics-com.myshopify.com",' | remove:'"\/\/app.shippingratescalculator.com\/js\/core\/main.min.js?timestamp=1661874001\u0026shop=designeroptics-com.myshopify.com"' }}
      {%- else -%}
        {{ content_for_header }}
      {%- endif -%}
      
    {%- else -%} 
      {% capture optimized_content_for_header %}{{ content_for_header }}{% endcapture %}
      {{ optimized_content_for_header | replace: ' href=" ', ' data-href=" ' | replace: " src=", " data-src=" | replace: "window.addEventListener('load', asyncLoad, false);", "window.addEventListener('doLoad', asyncLoad, false);" | replace: "DOMContentLoaded", "" }}
    {%- endif -%}

    {%- render 'layout__custom-code--head' -%}

    {%- if template == 'blog' -%}
        <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Sans:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;1,100;1,200;1,300;1,400;1,500;1,600;1,700&display=swap" rel="preload" as="style" rel="preload">
        <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Sans:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;1,100;1,200;1,300;1,400;1,500;1,600;1,700&display=swap" rel="stylesheet">
    {%- endif -%}
    
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;400&display=swap" rel="preload" as="style" rel="preload">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;400&display=swap" rel="stylesheet">

    
    {%- if request.design_mode == false -%}
      {{-'global-scss.css' | asset_url | preload_tag: as: 'style', type:"text/css" -}}  
      {{-'unicons.css' | asset_url | preload_tag: as: 'style', type:"text/css" -}}  
      {%- if settings.headerType == "mega-menu" -%}
        {{-'mega-menu.css' | asset_url | preload_tag: as: 'style', type:"text/css" -}}  
      {%- endif -%}
    {%- endif -%}
    <link rel="stylesheet" href="{{ 'global-scss.css' | asset_url }}" media="print" onload="this.media='all'">
    <link rel="stylesheet" href="{{ 'unicons.css' | asset_url }}" media="print" onload="this.media='all'">
    {%- if settings.headerType == "mega-menu" -%}
      <link rel="stylesheet" href="{{ 'mega-menu.css' | asset_url }}" media="print" onload="this.media='all'">
    {%- endif -%}
    {%- if template contains 'product' -%}
        {%- if template contains 'miscellaneous' or template contains 'watch' or template contains 'silhouette' -%}
          <link rel="stylesheet" href="{{ 'pdp-miscellenous.css' | asset_url }}" media="print" onload="this.media='all'">
        {%- else -%}
          <link rel="stylesheet" href="{{ 'pdp-style.css' | asset_url }}" media="print" onload="this.media='all'">
        {%- endif -%}  
    {% endif %}

    {%- render 'boost-pfs-style' -%}
  
	  {%- if dropinblog == 1 -%}{{ dropinblog_head_bottom }}{%- endif -%}
    {%- render 'wishlisthero-styles' -%}
    {% style %}
      .MuiDialog-root .MuiButtonBase-root{ font-size: 12px; }
      .MuiDialog-root .MuiButtonBase-root .MuiButton-label{ color:white; font-size: 12px; }
      .MuiDialog-root .MuiButtonBase-root .MuiSvgIcon-root{ width: 20px; height: 20px; }
      .MuiSnackbar-root .MuiSnackbarContent-message #message-id{ font-size:12px; }
      @media(min-width:1920px){
        .MuiGrid-spacing-xs-2 > .MuiGrid-item{ max-width: 25%; flex-basis: 25%; }
      }
    {% endstyle %}
    
    {%- unless settings.headHTML == blank -%}
        {{settings.headHTML}}
    {%- endunless -%}

    {%- render "header-scripts" -%}

  </head>
  <body class="template--{{template}} col__template--{{template.suffix}}"{% if customer %}data-user="true" data-useremail="{{ customer.email }}" data-userid="{{ customer.id }}" data-username="{{ customer.name }}"{% endif %} data-theme="contact-lens">
    
    <!-- commented because of non-sticky header TW#33513957  -->
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-NX9PSGP" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
    
    <div class="container">
      <div class="container__top-part">
          {% section 'top-info-bar' %}
          <div id="booster__popup--holder" class="booster__popup--holder"></div>

          {%- if template.suffix == 'no-header' or template.suffix == 'landing-no-header' -%}
          {%- elsif template.suffix == 'landing-alt-header' -%}
            {% section 'header-alt' %}
          {%- else -%}
            {%- case settings.headerType -%}
              {%- when 'default' -%}
                {% section 'header--default' -%}
              {%- when 'inline-search' -%}
                {%- section 'header--inline-search' -%}
              {%- when 'logo-top' -%}
                {%- section 'header--logo-top' -%}
              {%- when 'mega-menu' -%}  
                {%- section 'header--mega-menu' -%} 
            {%- endcase -%}
          {%- endif -%}
          {% section 'bottom-info-bar' %}
      </div> 
      <div id="quickbuy__modal" class="modal"><div onclick="closeModal(event)" class="overlay"></div><div class="content"></div></div>
      {%- if settings.recentlyBought -%}
        <a id="recently-bought--holder" class="recently-bought--holder {% unless settings.recentlyBoughtMobile -%}hide-mobile{%-endunless-%}">
          <div class="recently-bought--img-ratio"><div class="recently-bought--img"><img src=""></div></div>
          <div class="recently-bought--info">
            <div class="recently-bought--who"></div>
            <div class="recently-bought--what"></div>
            <div class="recently-bought--price"><span class="current-price"></span><span class="old-price"></span></div>
            <div class="recently-bought--when"></div>
          </div>
        </a>
        <script>
          const recentlyBoughtConfig = { useNames: {{settings.recentlyBoughtNames}}, nameFormat: '{{settings.recentlyBoughtNameFormat}}', allProductsUrl: '{{routes.all_products_collection_url}}' }
        </script>
      {%- endif -%}

      <div class="add-to-cart__success--holder modal">
        {%- unless settings.cartAction == 'message' -%}<div onclick="closeModal(event)" class="overlay add-to-cart__success--overlay"></div>{%- endunless -%}
        <div class="add-to-cart__success" id="checkout--success">
        {%- render 'add-to-cart__template--success' -%}
        </div>
      </div>
      {%- render 'layout__template--specific' -%}
      {%- comment -%} NK - index template added in below condition for slick {%- endcomment -%}
      <main class="main-content wrapper " id="MainContent" role="main">
        {{ content_for_layout }}
        <div class="wrapper--bottom">
          {% comment %}{% section 'info-badges' %}{% endcomment %}
          {% section 'footer' %}
          {% section 'subfooter' %}
        </div>
      </main>
    </div>

    {% render 'layout__app--code' %}
    {% render 'layout__bstr--constants' %}

    {%- unless settings.bodyCSS == blank -%}
      <style type="lazyload2">
        {{settings.bodyCSS}}
      </style>
    {%- endunless -%}

    {% render 'footer-scripts' %}

    {%- unless settings.bodyHTML == blank -%}
      {{settings.bodyHTML}}
    {%- endunless -%}
    
    {%- unless settings.bodyJS == blank -%}
      <script  type="lazyload2">
        {{settings.bodyJS}}
      </script>
    {%- endunless -%}

    {% render 'layout__custom-code--body' %}

    {% render 'custom-footer-scripts'%}

    <!--begin-boost-pfs-js-->
    {% render 'boost-pfs' %}
  	<!--end-boost-pfs-js-->

    <script src="{{ 'optimization.js' | asset_url }}" defer></script>
    <script enableOptimization="true" src="{{ 'optz-sh.js' | asset_url }}" defer></script>  
  </body>
</html>
