    {%- else -%}
      {{ header_captured}}
    {%- endif -%}
    {%- endcomment -%}
    {%- if template !='index' or request.host == "designeroptics-com.myshopify.com" or template contains 'cart' or template contains 'checkout' -%}
      {%- if template contains "collection"-%}
